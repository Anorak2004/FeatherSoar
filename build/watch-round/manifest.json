{
  "package": "com.vivo.feathersoar",
  "name": "轻羽飞扬",
  "versionName": "1.0.0",
  "versionCode": 1,
  "appCategory": [
    "sports"
  ],
  "icon": "/assets/images/logo.png",
  "features": [
    {
      "name": "system.router"
    },
    {
      "name": "blueos.app.sensor.accelerometer"
    },
    {
      "name": "blueos.app.sensor.gyroscope"
    },
    {
      "name": "blueos.app.health.heartrate"
    },
    {
      "name": "blueos.app.health.workout"
    },
    {
      "name": "blueos.app.notification"
    },
    {
      "name": "blueos.app.storage.database"
    }
  ],
  "deviceTypeList": [
    "watch-round",
    "watch-square"
  ],
  "config": {
    "designWidth": 466,
    "debug": true
  },
  "router": {
    "entry": "pages/Home",
    "pages": {
      "pages/Home": {
        "component": "index"
      },
      "pages/SessionStart": {
        "component": "index"
      },
      "pages/Dashboard": {
        "component": "index"
      },
      "pages/Report": {
        "component": "index"
      },
      "pages/History": {
        "component": "index"
      },
      "pages/Settings": {
        "component": "index"
      }
    }
  },
  "display": {
    "backgroundColor": "#33C9AB"
  },
  "minPlatformVersion": 1070
}